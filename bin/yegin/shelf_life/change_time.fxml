         <font>
            <Font name="HCR Dotum Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="93.0" layoutY="142.0" mnemonicParsing="false" onAction="#change" prefHeight="27.0" prefWidth="49.0" text="수정">
         <font>
            <Font name="HCR Dotum Bold" size="12.0" />
         </font></Button>
      <Button layoutX="160.0" layoutY="142.0" mnemonicParsing="false" onAction="#close" prefHeight="27.0" prefWidth="49.0" text="닫기">
         <font>
            <Font name="HCR Dotum Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
